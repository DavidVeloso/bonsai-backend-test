  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest .",
    "lint": "tslint --project '.'",
    "prettify": "prettier --config ./.prettierrc.json --write './source/**/*.ts'",
    "sane": "yarn lint && yarn prettify && yarn test",
    "start": "ts-node source/main.ts",
    "watch": "nodemon --watch 'source/**/*.ts' --ignore 'source/**/*.test.ts' --exec 'ts-node' source/main.ts dist/main.js",
    "build": "tsc"
  },
  "dependencies": {
    "apollo-server": "2.5.0",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "graphql": "14.3.0",
    "mongodb": "3.2.4",
    "mongoose": "5.5.7",
    "mongoose-paginate-v2": "^1.3.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "0.17.3",
    "typegoose": "5.6.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.18",
    "@types/mongodb": "3.1.26",
    "@types/mongoose": "5.5.0",
    "@types/mongoose-paginate-v2": "^1.0.5",
    "@types/node": "12.0.0",
    "@types/winston": "^2.4.4",
    "jest": "24.8.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  }
}
